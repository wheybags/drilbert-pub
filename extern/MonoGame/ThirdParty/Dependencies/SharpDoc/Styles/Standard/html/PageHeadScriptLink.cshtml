@model SharpDoc.TemplateContext
@using SharpDoc.Model
@helper Dump()
    {
    <link href="@(Param.LinkToSyntaxHighlighterCss)shCoreDefault.css" rel="stylesheet" type="text/css" />
    <link href="@(Param.LinkToCss)syntaxHighlighter.css" rel="stylesheet" type="text/css" />
    <link href="@(Param.LinkToCss)sharpdoc.css" rel="stylesheet" type="text/css" />

    <script src="@(Param.LinkToJs)mootools.js" language="javascript"></script>
    <script src="@(Param.LinkToJs)mootools-more.js" language="javascript"></script>
    <script src="@(Param.LinkToSyntaxHighlighterJs)shCore.js" type="text/javascript"></script>
    <script src="@(Param.LinkToSyntaxHighlighterJs)shBrushCpp.js" type="text/javascript"></script>
    <script src="@(Param.LinkToSyntaxHighlighterJs)shBrushCSharp.js" type="text/javascript"></script>
    <script src="@(Param.LinkToSyntaxHighlighterJs)shBrushJScript.js" type="text/javascript"></script>
    <script src="@(Param.LinkToSyntaxHighlighterJs)shBrushVb.js" type="text/javascript"></script>
    <script src="@(Param.LinkToSyntaxHighlighterJs)shBrushPlain.js" type="text/javascript"></script>
    <script src="@(Param.LinkToSyntaxHighlighterJs)shBrushXml.js" type="text/javascript"></script>
    <script type="text/javascript" language="javascript">
        SyntaxHighlighter.defaults['gutter'] = false;
        SyntaxHighlighter.defaults['smart-tabs'] = false;
        SyntaxHighlighter.all();
    </script>
    <script src="@(Param.LinkToJs)sharpdoc.js" language="javascript"></script>            

        
    <meta name="Microsoft.Help.Id" content="@Model.Topic.Id" />
    <meta name="Microsoft.Help.SelfBranded" content="true" />
    <meta name="Microsoft.Help.Locale" content="en-us" />
    <meta name="Microsoft.Help.F1" content="@Escape(Model.Topic.FullName)" />
    <meta name="Microsoft.Help.Category" content="TargetOS:Windows" />
    <meta name="Microsoft.Help.Category" content="DevLang:VB" />
    <meta name="Microsoft.Help.Category" content="DevLang:CSharp" />
    <meta name="Microsoft.Help.Category" content="DevLang:FSharp" />
    if (Model.Topic.Parent != null)
    {
        <meta name="Microsoft.Help.TocParent" content="@Model.Topic.Parent.Id" />
        <meta name="Microsoft.Help.TocOrder" content="-1" />
    }
    else
    {
        <meta name="Microsoft.Help.TocParent" content="-1" />
        <meta name="Microsoft.Help.TocOrder" content="-1" />  
    }
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
}



